cp /code/JavaWeb/jstl/target/classes/log4j2.xml /usr/local/tomcat/webapps/jstl/WEB-INF/
cp /code/JavaWeb/jstl/target/classes/context.xml /usr/local/tomcat/webapps/jstl/META-INF/
